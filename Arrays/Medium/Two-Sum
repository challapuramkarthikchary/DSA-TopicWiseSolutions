class Solution {
    public int[] twoSum(int[] nums, int target) {

        HashMap<Integer, Integer> a = new HashMap<>();
        int ans[] = new int[2];
        for(int i=0;i<nums.length;i++){
            int complimentary = target - nums[i];
            if(a.containsKey(complimentary)){
                ans[0]=i;
                ans[1]=a.get(complimentary);
                return ans;
            }
            a.put(nums[i],i);
        }  
        return ans;
      
    }
}
