class Solution {
    public int maxSubArray(int[] nums) {
        
        //kadane's Algo
        
        int sum=0;
        int maxsum = nums[0];
        for(int i=0;i<nums.length;i++){
             if(sum<0){
                sum=0;
            }
            sum+=nums[i];
            maxsum = Math.max(sum,maxsum);
        }

        return maxsum;

    }
}
