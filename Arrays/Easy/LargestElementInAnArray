import java.util.*;
import java.lang.*;
import java.io.*;

public class Main
{
	public static void main (String[] args) throws java.lang.Exception
	{
		//largest element in an array

		int arr[] = {5,3,1,2,4,7,6,2};

		int largest = arr[0];

		for(int i=1; i<arr.length; i++){
			if(arr[i] > largest){
				largest=arr[i];
			}
		}
		
		System.out.println(largest);

	}
}
