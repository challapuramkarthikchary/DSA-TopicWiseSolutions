485. Max Consecutive Ones
__________________________________________________________
class Solution {
    public int findMaxConsecutiveOnes(int[] nums) {
        
        int count=0;
        int maxcount=0;

        for(int num: nums){
            if(num==1){
                count++;
            }else{
                maxcount=Math.max(count,maxcount);
                count=0;
            }
        }
        maxcount=Math.max(count,maxcount);
        return maxcount;

    }
}
